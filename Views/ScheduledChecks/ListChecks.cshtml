@model List<UptimeMonitor.Models.ScheduledCheck>

<div class="list-group">
  @if (Model.Count == 0)
  {
    <div class="list-group-item list-group-item-action">
      No scheduled checks found.
    </div>
  }
  else
  {
    foreach (var check in Model)
    {
      <div class="list-group-item list-group-item-action">
        @check.Domain
      </div>
    }
  }
</div>
